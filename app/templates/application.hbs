{{#link-to 'index'}}
  {{#kit-search-result
    title="title"
    subtitle="Search Result with img, title, subtitle, and play icon"
    as |sr|
  }}
    {{#yield-for sr 'left-content'}}
      <img src="https://unsplash.it/42/24?random"/>
    {{/yield-for}}

    {{#yield-for sr 'right-content'}}
        {{kit-icon name='play'}}
    {{/yield-for}}
  {{/kit-search-result}}
{{/link-to}}

